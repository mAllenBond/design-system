{
  "name": "design-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "check:lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "test": "vitest run",
    "check:all": "yarn check:lint && yarn check:prettier && tsc --noEmit && yarn test",
    "fix:lint": "yarn check:lint --fix",
    "check:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "fix:prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "fix:all": "yarn fix:lint && yarn fix:prettier",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@fontsource/material-icons": "^5.0.18",
    "@fontsource/roboto": "^5.0.13",
    "itertools": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.7.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.16.7",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
